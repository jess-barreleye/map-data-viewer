# InfluxDB Configuration for OpenRVDAS
INFLUXDB_URL=http://localhost:8086
INFLUXDB_TOKEN=your-influxdb-token-here
INFLUXDB_ORG=your-org
INFLUXDB_BUCKET=openrvdas

# Server Configuration
PORT=8086
WS_PORT=8086

# Data Source
USE_MOCK_DATA=true

# Query Settings
QUERY_INTERVAL_MS=1000
HISTORICAL_RANGE_HOURS=24

# OpenRVDAS Integration
# If using file-based data instead of InfluxDB
DATA_FILE_PATH=/opt/openrvdas/data
