# WebSocket Server Configuration
WS_PORT_MULTIBEAM=8085

# InfluxDB Configuration
INFLUXDB_URL=http://localhost:8086
INFLUXDB_TOKEN=your-token-here
INFLUXDB_ORG=your-org-name
INFLUXDB_BUCKET=multibeam-data

# Query interval in milliseconds
QUERY_INTERVAL=1000

# Use mock data instead of InfluxDB
USE_MOCK_DATA=true
