# WebSocket Server Configuration
WS_PORT_TELEMETRY=8084

# InfluxDB Configuration
INFLUXDB_URL=http://localhost:8086
INFLUXDB_TOKEN=your-token-here
INFLUXDB_ORG=your-org-name
INFLUXDB_BUCKET=rov-data

# Query interval in milliseconds (how often to poll InfluxDB)
QUERY_INTERVAL=1000

# Use mock data instead of InfluxDB (set to 'false' when InfluxDB is configured)
USE_MOCK_DATA=true
